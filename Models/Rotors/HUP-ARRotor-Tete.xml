	<path>Models/hup-arrotor-tete.ac</path>

	<texture-path>Models/Textures</texture-path>
        <offsets>
            <heading-deg>180</heading-deg>
        </offsets>

        <animation>
                <condition>
                    <equals>
                        <property>sim/current-view/view-number</property>
                        <value>999</value>
                    </equals>
            </condition>
		<type>select</type>
                <object-name>Gabarit-Pose</object-name>
 	</animation>


        <animation>
		<type>rotate</type>
                <object-name>Rotor-Tete</object-name>
                <property>sim/multiplay/generic/float[3]</property>
		<factor>1</factor>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>



</PropertyList>